{
    "name": "php-curl-class/php-curl-class",
    "description": "PHP Curl Class makes it easy to send HTTP requests and integrate with web APIs.",
    "homepage": "https://github.com/php-curl-class/php-curl-class",
    "license": "Unlicense",
    "keywords": [
        "php", "curl", "class", "api", "api-client", "client", "framework",
        "http", "http-client", "http-proxy", "json", "proxy", "requests",
        "rest", "restful", "web service", "xml"
    ],
    "authors": [
        {
            "name": "Zach Borboa"
        }
    ],
    "require": {
        "php": ">=5.3",
        "ext-curl": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "ext-gd": "*",
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Curl\\": "src/Curl/"
        }
    }
}
